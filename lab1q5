import java.util.Scanner;

public class Q5 {
    public static void main(String args[]) {
        Scanner s = new Scanner(System.in);

        System.out.print("Enter five digit number: ");
        int n = s.nextInt();

        System.out.println(
            (n / 10000) + "   " +
            (n / 1000 % 10) + "   " +
            (n / 100 % 10) + "   " +
            (n / 10 % 10) + "   " +
            (n % 10)
        );
    }
}
